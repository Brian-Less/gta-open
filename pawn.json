{
	"user": "Patrick",
	"repo": "gta-open",
	"entry": "gamemodes/main.pwn",
	"output": "gamemodes/main.amx",
	"dependencies": [
		"sampctl/samp-stdlib",
		"https://github.com/maddinat0r/sscanf",
		"https://github.com/pawn-lang/YSI-Includes/tree/5.x",
		"https://github.com/Zeex/amx_assembly",
		"https://github.com/Y-Less/code-parse.inc",
		"https://github.com/Y-Less/indirection",
		"https://github.com/Southclaws/formatex",
		"https://github.com/Zeex/samp-plugin-crashdetect",
		"https://github.com/pBlueG/SA-MP-MySQL",
		"https://github.com/Sreyas-Sreelal/samp-bcrypt",
		"https://github.com/Southclaws/samp-logger",
		"https://github.com/PatrickGTR/MySQL-Prepared-Statements",
		"https://github.com/samp-incognito/samp-streamer-plugin"
	],
	"local": true,
	"builds": [
		{
			"name": "",
			"args": [
				"-;+",
				"-(+",
				"-d3",
				"-E"
			],
			"includes": [
				"gamemodes/utils",
				"gamemodes/core",
				"gamemodes/core/user-interface",
				"gamemodes/core/chat",
				"gamemodes/core/anti-cheat",
				"gamemodes/core/server",
				"gamemodes/core/account",
				"gamemodes/core/player",
				"gamemodes/core/player/attachment",
				"gamemodes/core/admin",
				"gamemodes/core/systems",
				"gamemodes/core/Commands"
			],
			"constants": {
				"DEV_BUILD": "1"
			}
		}
	],
	"runtime": {
		"version": "0.3.7",
		"mode": "server",
		"echo": "-",
		"rcon_password": "password",
		"port": 7777,
		"hostname": "SA-MP Server",
		"maxplayers": 50,
		"language": "",
		"mapname": "San Andreas",
		"weburl": "www.sa-mp.com",
		"gamemodetext": "Unknown",
		"announce": true,
		"lanmode": false,
		"query": true,
		"rcon": false,
		"logqueries": false,
		"sleep": 5,
		"maxnpc": 0,
		"stream_rate": 1000,
		"stream_distance": 200,
		"onfoot_rate": 30,
		"incar_rate": 30,
		"weapon_rate": 30,
		"chatlogging": true,
		"timestamp": true,
		"messageholelimit": 3000,
		"messageslimit": 500,
		"ackslimit": 3000,
		"playertimeout": 10000,
		"minconnectiontime": 0,
		"lagcompmode": 1,
		"connseedtime": 300000,
		"db_logging": false,
		"db_log_queries": false,
		"conncookies": true,
		"cookielogging": false,
		"output": true
	}
}